{
  "database": {
    "rules": "dayabase.rules.JSON"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.JSON"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
